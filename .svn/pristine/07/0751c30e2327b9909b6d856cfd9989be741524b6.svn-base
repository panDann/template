html,
body {
    font-size: 0.44rem;
    font-family: Helvetica Neue, Helvetica, PingFang SC, Hiragino Sans GB, Microsoft YaHei, SimSun, sans-serif;
}

.gx-container{
    margin-bottom: 2rem;
}

/* 操作弹窗 */
.gx-modal-overlay {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.4);
    z-index: 10600;
    -webkit-transition-duration: 400ms;
    transition-duration: 400ms;
    font-size: 0.58rem;
    display: none; }
.gx-modal-overlay .actions-modal {
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 11000;
    width: 100%;
    -webkit-transition-duration: 300ms;
    transition-duration: 300ms;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0); }
.gx-modal-overlay .actions-modal .actions-modal-group {
    margin: 0.28rem; }
.gx-modal-overlay .actions-modal .actions-modal-button {
    text-align: center;
    font-weight: normal;
    background: #fff;
    display: block;
    color: #2e74f9;
    padding: 0.43rem 0; }
.gx-modal-overlay .actions-modal .actions-modal-button + .actions-modal-button {
    border-top: 1px solid #ccd3dc; }
.gx-modal-overlay .actions-modal .actions-modal-button:first-child {
    border-radius: 0.4rem 0.4rem 0 0; }
.gx-modal-overlay .actions-modal .actions-modal-button:last-child {
    border-radius: 0 0 0.4rem 0.4rem; }
.gx-modal-overlay .actions-modal .actions-modal-button:first-child:last-child {
    border-radius: 0.4rem; }
.gx-modal-overlay.active .actions-modal {
    -webkit-transition-duration: 300ms;
    transition-duration: 300ms;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0); }

/* 对话框 */
.gx-pop-overlay {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.4);
    display: none; }

.gx-dialog-modal {
    width: 9.04rem;
    position: absolute;
    left: 50%;
    margin-left: -4.52rem;
    border-radius: 0.2rem;
    background-color: #fff;
    top: 6.28rem;
    text-align: center; }
.gx-dialog-modal .dialog-modal-inner {
    padding-top: 0.88rem;
    padding-bottom: 0.5rem;
    margin: 0 0.37rem;
    color: #191919;
    line-height: 1;
    border-bottom: 1px solid #ecedf3; }
.gx-dialog-modal .dialog-modal-inner .dialog-modal-title {
    font-size: 0.44rem;
    margin-bottom: 0.22rem;
    display: flex;
    justify-content: center;
    align-items: center; }
.gx-dialog-modal .dialog-modal-inner .dialog-modal-title .icon {
    width: 0.73rem;
    height: 0.73rem;
    background-size: 100% 100%;
    background-position: left top;
    margin-right: 0.17rem; }
.gx-dialog-modal .dialog-modal-inner .dialog-modal-title .icon.fail {
    background-image: url("../images/icon_close.png"); 
}
.gx-dialog-modal .dialog-modal-inner .dialog-modal-text {
    font-size: 0.36rem;
    line-height: 1.4; }
.gx-dialog-modal .dialog-modal-buttons {
    padding: 0.5rem 0;
    display: flex;
    align-items: center;
    justify-content: center; }
.gx-dialog-modal .dialog-modal-buttons .modal-btn {
    width: 3.6rem;
    height: 1.09rem;
    background-color: #2e74f9;
    color: #fff;
    font-size: 0.48rem;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: .2rem; }
.gx-dialog-modal .dialog-modal-buttons .modal-btn + .modal-btn {
    margin-left: .25rem; }

/* 头部 */
.gx-header {
    height: 1.5rem;
    position: absolute;
    right: 0;
    left: 0;
    z-index: 10;
    height: 1.5;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #191919;
    /* 默认白色头部 */
    /* 透明头部 */
    /* 蓝色头部 */ }
.gx-header h1.title {
    position: absolute;
    font-size: 0.5rem;
    text-align: center;
    white-space: nowrap;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: normal; }
.gx-header .button-nav {
    height: 100%;
    display: flex;
    align-items: center;
    padding: 0 0.29rem;
    position: relative;
    z-index: 2;
    font-size: 0.36rem; }
.gx-header .button-nav.operate {
    color: rgba(255, 255, 255, 0.75); }
.gx-header .button-nav.back:before {
    font-family: "CTwing";
    content: "\e617";
    font-size: 0.45rem; }
.gx-header.gx-header-default {
    background-color: #fff;
    border-bottom: 1px solid #ccd3dc; }
.gx-header.gx-header-transparent {
    background: transparent;
    border: none;
    color: #3c3c3c; }
.gx-header.gx-header-blue {
    background-color: #2e74f9;
    color: #fff;
    border: none; }

/* 底部 */
.gx-footer {
    height: 1.44rem;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: #fff;
    z-index: 10;
    display: flex;
    justify-content: space-between;
    padding: 0 0.42rem;
    font-size: 0.29rem; }
.gx-footer .gx-footer-item {
    width: 2.16rem;
    height: 100%;
    display: flex;
    font-size: 0.1rem;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #7E848B; }
.gx-footer .gx-footer-item .active+p {
    color: #FF8C01; }
.gx-footer .gx-footer-item > i {
    height: 0.6rem;
    font-size: 0.59rem;
    margin-bottom: 0.08rem;}

